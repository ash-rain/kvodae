<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
	<msapplication>
		<tile>
			<square70x70logo src="http://microbrander.com/img/tiny.png"/>
			<square150x150logo src="http://microbrander.com/img/square.png"/>
			<wide310x150logo src="http://microbrander.com/img/wide.png"/>
			<square310x310logo src="http://microbrander.com/img/large.png"/>
			<TileColor>#00ccff</TileColor>
		</tile>
		<notification>
			<polling-uri src="http://microbrander.com/notifications/rss"/>
			<frequency>30</frequency>
			<cycle>1</cycle>
		</notification>
	</msapplication>
</browserconfig>